# Copy this file to .env and update the values

# MongoDB Configuration
MONGO_URI=mongodb://mongodb:27017/minecraft-panel
MONGO_DB_NAME=minecraft-panel

# Default Admin Credentials (Created on first startup if no users exist)
DEFAULT_ADMIN_USERNAME=admin
DEFAULT_ADMIN_PASSWORD=ChangeMe123!

# JWT Secret (Change this to a random string!)
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# Gemini AI API Key (for AI command assistance)
GEMINI_API_KEY=your-gemini-api-key-here

# Server Configuration
PORT=3000
NODE_ENV=development

# Minecraft Server Settings
MC_SERVER_BASE_PATH=/app/servers
MC_PORT_RANGE_START=25565
MC_PORT_RANGE_END=25575
MC_DEFAULT_MEMORY=2048
